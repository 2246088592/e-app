<template name="request">
  <view class="flex">
    <view class="w-10 flex flex-col justify-center items-center mr-2">
      <image class="h-8 w-8 circle" mode="scaleToFill" src="{{person.avatar}}"/>
      <text class="color-label text-xs">{{person.name}}</text>
    </view>
    <view>
      <view>
        <text class="color-label">请购单编码：</text>
        <text class="color-bg-title">{{code}}</text>
      </view>
      <view>
        <text class="color-label">申请日期：</text>
        <text class="color-bg-title">{{date}}</text>
      </view>
      <view>
        <text class="color-label">申请部门：</text>
        <text class="color-bg-title">{{dept}}</text>
      </view>
      <view>
        <text class="color-label">申请用途：</text>
        <text class="color-bg-title">{{remark}}</text>
      </view>
    </view>
  </view>
</template>
