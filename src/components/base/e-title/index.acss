.title-wrapper>.title {
	position: relative;
	display: flex;
	padding: 16rpx;
	height: 100rpx;
	align-items: center;
	justify-content: space-between;
}

.title-wrapper>.title:after {
	position: absolute;
	right: 0;
	bottom: 0;
	left: 16rpx;
	display: block;
	height: 1px;
	background-color: rgba(0,0,0,.1);
	content: '';
	transform: scaley(.5);
}
