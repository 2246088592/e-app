<view class="form-wrapper full-parent">
  <slot name="header"></slot>
  <scroll-view style="flex: 1;" scroll-y="{{true}}">
    <form onSubmit="handleSubmit" onReset="handleReset">
      <block a:for="{{bizObj}}">
        <e-input a:if="{{item.component === 'e-input'}}" model="{{item}}" onValidate={{'on_'+item.formId+'_'+item.key}}></e-input>
        <e-address a:elif="{{item.component === 'e-address'}}" model="{{item}}" onValidate={{'on_'+item.formId+'_'+item.key}}></e-address>
        <e-cascader a:elif="{{item.component === 'e-cascader'}}" model="{{item}}" onValidate={{'on_'+item.formId+'_'+item.key}}></e-cascader>
        <e-date-picker a:elif="{{item.component === 'e-date-picker'}}" model="{{item}}" onValidate={{'on_'+item.formId+'_'+item.key}}></e-date-picker>
        <e-dept-chooser a:elif="{{item.component === 'e-dept-chooser'}}" model="{{item}}" onValidate={{'on_'+item.formId+'_'+item.key}}></e-dept-chooser>
        <e-picker a:elif="{{item.component === 'e-picker'}}" model="{{item}}" onValidate={{'on_'+item.formId+'_'+item.key}}></e-picker>
      </block>
      <view class="form-btn">
        <button formType="submit">Submit</button>
        <button formType="reset">Reset</button>
      </view>
    </form>
  </scroll-view>
  <slot name="footer"></slot>
</view>